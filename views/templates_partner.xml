<odoo>
    <data>
        <!-- Dar de alta en res.partner -->
        <record model="res.partner" id="partner agorasi">
            <field name="name">agorasi</field>
            <field name="display_name">agorasi display</field>
            <field name="email">agorasi@agorasi.com </field>
            <field name="active">true</field>
            <field name="company_id">1</field>
            <field name="lang">es_ES</field>
        </record>
        <record model="res.partner" id="partner agoratamen">
            <field name="name">agoratamen</field>
            <field name="display_name">agoratamen display</field>
            <field name="email">agoratamen@agoratamen.com </field>
            <field name="active">true</field>
            <field name="company_id">1</field>
            <field name="lang">es_ES</field>
        </record>
        <record model="res.users" id="users agoraUsuario">
            <field name="name">agoraUsuario</field>
            <field name="display_name">agoraUsuario display</field>
            <field name="email">agoraUsuario@agoraUsuario.com</field>
            <field name="active">true</field>
            <field name="company_id">1</field>
            <field name="lang">es_ES</field>
            <field name="login">agoraUsuario@agoraUsuario.com</field>
            <field name="groups_id" eval="[(6, 0, [ref('openacademy.grupo_usuario')])]"/>
            <!--A relación many2many lévase na táboa res_groups_users_rel
            obtemos o valor a partir do id externo
            O nome do campo relación many2many https://useopenerp.com/v8/model/res-users    -->
        </record>
    </data>
</odoo>